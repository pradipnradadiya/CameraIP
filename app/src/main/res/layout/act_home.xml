<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".home.activity.HomeAct">

    <com.etebarian.meowbottomnavigation.MeowBottomNavigation
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:mbn_backgroundBottomColor="@color/bottom_navigation_background_color"
            app:mbn_countBackgroundColor="@color/red"
            app:mbn_countTextColor="@color/white"
            app:mbn_defaultIconColor="@color/default_icon_color"
            app:mbn_rippleColor="@color/ripple_color"
            app:mbn_selectedIconColor="@color/selected_icon_color"
            app:mbn_shadowColor="@color/shadow_color"
            app:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/bottomNavigation"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

    <include android:layout_width="0dp"
             android:layout_height="wrap_content"
             layout="@layout/toolbar"
             app:layout_constraintEnd_toEndOf="parent"
             app:layout_constraintStart_toStartOf="parent"
             app:layout_constraintTop_toTopOf="parent"
             android:id="@+id/include"/>


    <FrameLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@+id/include"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/bottomNavigation"
            android:id="@+id/frameContainer">


    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>